(let p (smt-bool-const "p"))
(check (= (smt-bool-const "p") (smt-bool-const "p")))
(check (!= (smt-bool-const "p") (smt-bool-const "q")))

(check (unsat (and p (not p))))
(fail (check (unsat (and p p))))

(let x (smt-int-const "x"))
(let y (smt-int-const "y"))

(check (unsat (smt-= x (+ x (smt-int 1)))))
(check (unsat (smt-= (- x y) (+ (- x y) (smt-int 1)))))


(let x (smt-fn-int "x" "Int"))
(let t (smt-fn-int "t" "Int"))
(let y (smt-fn-int "y" "Int"))

(let cond1 (smt-= (smt-call x (smt-int 0)) (smt-call t (smt-int 0))))
(let cond2 (smt-= (smt-call y (smt-int 1)) (smt-call t (smt-int 0))))
(let cond3 (smt-= (smt-call x (smt-int 1)) (smt-call y (smt-int 1))))

(fail (check (unsat cond1 cond2 cond3)))
